    "last 1 Chrome version"
  ],
  "dependencies": {
    "@kubernetes/client-node": "^0.11.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "event-iterator": "^1.2.0",
    "google-protobuf": "^3.11.1",
    "grpc": "^1.24.2",
    "grpc-web": "^1.0.7",
    "parcel": "^1.12.4",
    "protobufjs": "^6.8.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
